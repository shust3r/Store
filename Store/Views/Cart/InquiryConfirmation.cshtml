@model Store_Models.OrderHeader

<div class="text-center">

    @if (Model!=null)
    {
        <h1 class="text-info"> Order Created</h1>
        <p>Order #@Model.Id was created successfully</p>
    }
    else
    {
        <h1 class="text-info">Inquiry Confirmation</h1>

        <p>We have received your inquiry and will contact you soon!</p>
    }
    <img src="/images/patio.jpg" width="80%" />
</div>